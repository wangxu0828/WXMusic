<!-- 输入框 -->
<van-search background="#fafafa" shape="round" placeholder="请输入喜欢的音乐" bind:click-input="handleSearchClick" disabled />
<!-- 轮播图 -->
<swiper class="swiper" indicator-dots style="height: {{swiperHeight}}px;">
  <block wx:for="{{banners}}" wx:key="bannerId">
    <swiper-item class="swiper-item">
      <image class="image-item" src="{{item.pic}}" mode="widthFix" />
    </swiper-item>
  </block>
</swiper>