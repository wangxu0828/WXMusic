page {
  padding-left:20px;
  padding-right:20px;
  background-color: #f7f7f7;
}

.search {
  width: 750rpx;
  position: relative;
  left: -40rpx;
}

.keywords {
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}

.keywords .keyWord {
  background-color: #fff;
  margin-right: 40rpx;
  padding: 15rpx;
  border-radius: 40rpx;
  margin-bottom: 20rpx;
  font-weight: 500;
}

.keywords .keyWord:first-of-type {
  color: #229c72;
}

.search-suggest .title {
  color: #229c72;
  font-weight: 600;
}

.search-suggest .suggest-item {
  display: flex;
  align-items: center;
}

.search-suggest .suggest-item .search-icon{
  width: 60rpx;
  height: 60rpx;
}

.search-suggest .suggest-item .name{
}
